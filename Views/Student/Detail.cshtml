@model Student
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
   <table class="table">
       <thead>
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">FirstName</th>
                    <th scope="col">LastName</th>
                    <th scope="col">Email</th>
                    <th scope="col">Age</th>
                   
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td >@Model.Id</td>
                    <td >@Model.FirstName</td>
                    <td >@Model.LastName</td>
                    <td >@Model.Email</td>
                    <td >@Model.Age</td>
                </tr>
            </tbody>
        </table>
</body>
</html>